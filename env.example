# Frontend Configuration
VITE_NEWRELIC_LICENSE_KEY=your_newrelic_license_key_here
VITE_NEWRELIC_APPLICATION_ID=winette-api
VITE_API_BASE_URL=http://127.0.0.1:3001

# Backend Configuration
PORT=3001
NODE_ENV=development

# Database Configuration (Supabase)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_STORAGE_BUCKET=label-images

# Cache Configuration (Upstash)
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# Task Queue Configuration (QStash)
QSTASH_URL=https://qstash.upstash.io
QSTASH_TOKEN=your_qstash_token_here
QSTASH_CURRENT_SIGNING_KEY=your_qstash_current_signing_key_here
QSTASH_NEXT_SIGNING_KEY=your_qstash_next_signing_key_here

# API Base URL for QStash webhooks
# For local development: use localhost (jobs will be processed directly to bypass QStash loopback restrictions)
# For production: use your deployed domain (e.g., https://your-app.vercel.app)
API_BASE_URL=http://localhost:3001

# Rate Limiting Configuration
# Set to 'true' to disable rate limiting for local development
DISABLE_RATE_LIMITING=false

# Observability Configuration (NewRelic)
NEW_RELIC_LICENSE_KEY=your_newrelic_license_key_here
NEW_RELIC_APP_NAME=winette-api
NEW_RELIC_NO_CONFIG_FILE=true

# AI Model Configuration
# Required for production label generation with DALL-E 3 and GPT-5
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override default language model
# Default: claude-3-5-haiku-20241022
LANGCHAIN_DEFAULT_MODEL=claude-3-5-haiku-20241022

# Optional: Override model temperature settings for fine-tuning
# LANGCHAIN_DESIGN_TEMP=0.3
# LANGCHAIN_PROMPT_TEMP=0.7
# LANGCHAIN_LAYOUT_TEMP=0.2
# LANGCHAIN_REFINE_TEMP=0.1

# Debug Endpoints (for testing only)
# Required to access debug endpoints like /api/test-langchain-pipeline
# Keep this secure and only enable in development/staging
DEBUG_TOKEN=your_secure_debug_token_here
ENABLE_DEBUG_ENDPOINTS=false

LANGSMITH_TRACING="true"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT="winette"